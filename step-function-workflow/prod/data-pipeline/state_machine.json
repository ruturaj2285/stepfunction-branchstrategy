{
  "Comment": "POC data pipeline (prod)",
  "StartAt": "InvokeReportJob",
  "States": {
    "InvokeReportJob": {
      "Type": "Task",
      "Resource": "arn:aws:states:::lambda:invoke",
      "Parameters": {
        "FunctionName": "report-job",
        "Payload.$": "$"
      },
      "ResultSelector": {
        "lambda.$": "$.Payload"
      },
      "End": true
    }
  }
}
